name "nanoc"
version "0.0.1"
description "Nano library C"
authors "Yuriy Moskoff"
copyright "Copyright Â© 2024, Yuriy Moskoff"
license "LGPL-3.0-or-later"
targetType "dynamicLibrary"
dflags "-nolibc" "-nodefaultlibs" "-fno-stack-protector" "-ftransition=tls" "-nostartfiles" platform="gdc"
sourceFiles "obj/raw_syscall.o" "obj/crt.o"
preBuildCommands "as source/raw_syscall.s -o obj/raw_syscall.o -c" "as crt/*.s -o obj/crt.o"
buildOptions "betterC"
versions "DISABLE_METADATA"
